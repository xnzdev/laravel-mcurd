{
    "name": "xnzdev/laravel-mcurd",
    "authors": [
        {
            "name": "chen",
            "email": "xnzdev@qq.com"
        }
    ],
    "keywords": ["laravel", "gii", "code generator", "GUI","CRUD"],
    "license":"MIT",
    "description": "visual code generator tools gii for laravel",
    "require": {
        "sebastian/diff": "^3.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.8|^4.0"
    },
    "autoload": {
        "psr-4":{
            "XnzDev\\MCurd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XnzDev\\MCurd\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XnzDev\\MCurd\\Providers\\GiiServiceProvider"
            ]
        }
    }
}
